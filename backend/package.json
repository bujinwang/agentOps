{
  "name": "realestate-crm-api",
  "version": "1.0.0",
  "description": "High-performance Express.js API for Real Estate CRM",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "start:ts": "node dist/server.js",
    "dev:ts": "nodemon --exec ts-node src-ts/server.ts",
    "build:ts": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ts": "jest --config jest.config.ts",
    "test:ts:watch": "jest --config jest.config.ts --watch",
    "test:ts:coverage": "jest --config jest.config.ts --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:ts": "eslint src-ts/ --ext .ts",
    "docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o swagger.json",
    "migrate": "node src/migrations/cli.js migrate",
    "migrate:status": "node src/migrations/cli.js status",
    "migrate:rollback": "node src/migrations/cli.js rollback",
    "migrate:create": "node src/migrations/cli.js create",
    "migrate:validate": "node src/migrations/cli.js validate"
  },
  "keywords": [
    "real-estate",
    "crm",
    "express",
    "api",
    "postgresql"
  ],
  "author": "Real Estate CRM Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.6.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.19.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
